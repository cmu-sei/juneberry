{
    "$schema": "https://json-schema.org/draft-07/schema",
    "type": "object",
    "properties": {
        "format_version": { "type": "string" },
        "options": {
            "type": "object",
            "properties": {
                "dataset": {
                    "type": "object",
                    "properties": {
                        "classes": { "type": "object" },
                        "config": { "type": "string" },
                        "histogram": { "type": "object" }
                    },
                    "required": [ "config" ]
                },
                "model": {
                    "type": "object",
                    "properties": {
                        "hash": { "type": "string" },
                        "name": { "type": "string" },
                        "num_classes": { "type": "number" }
                    },
                    "required": [ "name" ]
                }
            },
            "required": [ "dataset", "model" ]
        },
        "results": {
            "type": "object",
            "properties": {
                "classifications": {
                    "type": "array",
                    "items": {
                        "type": "object",
                        "properties": {
                            "file": { "type": "string" },
                            "actualLabel": { "type": "number" },
                            "actualLabelName": { "type": "string" },
                            "predictedClasses": {
                                "type": "array",
                                "items": {
                                    "type": "object",
                                    "properties": {
                                        "label": { "type": "number" },
                                        "labelName": { "type": "string" },
                                        "confidence": { "type": "number" }
                                    }
                                }
                            }
                        }
                    }
                },
                "labels": {
                    "type": "array",
                    "items": { "type": "number" }
                },
                "metrics": {
                    "type": "object",
                    "properties": {
                        "accuracy": { "type": "number" },
                        "balanced_accuracy": { "type": "number" },
                        "bbox": { "type": "object" },
                        "bbox_per_class": { "type": "object" }
                    },
                    "required": [ ]
                },
                "predictions": {
                    "type": "array",
                    "items": {
                        "type": "array",
                        "items": { "type": "number" }
                    }
                }
            },
            "required": [ ]
        },
        "times": {
            "type": "object",
            "properties": {
                "duration": { "type": "number" },
                "end_time": { "type": "string" },
                "start_time": { "type": "string" }
            },
            "required": [ ]
        }
    },
    "required": [
        "options",
        "results"
    ]
}
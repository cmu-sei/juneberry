{
    "batch_size": 256,
    "description": "Simple unit test demonstrating torchvision unit test models.",
    "epochs": 5,
    "evaluation_output": "juneberry.pytorch.evaluation.default.DefaultEvaluationOutput",
    "evaluation_procedure": "juneberry.pytorch.evaluation.default.DefaultEvaluationProcedure",
    "evaluation_transforms": [
        {
            "fqcn": "juneberry.transforms.random_crop_mirror.RandomCropMirror",
            "kwargs": {
                "height_pixels": 4,
                "mirror": 1,
                "width_pixels": 4
            }
        },
        {
            "fqcn": "juneberry.transforms.debugging_transforms.SizeCheckImageTransform",
            "kwargs": {
                "height": 28,
                "mode": "L",
                "width": 28
            }
        },
        {
            "fqcn": "torchvision.transforms.ToTensor"
        }
    ],
    "format_version": "0.2.0",
    "model_architecture": {
        "args": {
            "channels": 1,
            "img_height": 28,
            "img_width": 28,
            "num_classes": 10
